{
  "name": "Debrief Codespace",
  "image": "mcr.microsoft.com/devcontainers/universal",
  "postCreateCommand": "cd /workspaces/future-debrief/vs-code && yarn install && yarn run package:vsix && code --install-extension debrief-vscode.vsix",
  "workspaceFolder": "/workspaces/future-debrief/vs-code/workspace",
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.startupEditor": "readme",
        "workbench.sideBar.location": "left",
        "terminal.integrated.defaultProfile.linux": "bash"
      },
      "extensions": [
        "ms-vscode.vscode-typescript-next"
      ]
    }
  },
  "features": {},
  "remoteUser": "codespace",
  "forwardPorts": [],
  "portsAttributes": {}
}