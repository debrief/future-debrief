{
  "name": "future-debrief",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "get-ready": "pnpm install && pnpm build && cd libs/tool-vault-packager && npm install && pip install -r requirements.txt && npm run build && cd ../../",
    "clean": "rm -rf .turbo && turbo run clean",
    "build:shared-types": "pnpm --filter @debrief/shared-types build",
    "build:web-components": "pnpm --filter @debrief/web-components build",
    "build:vs-code": "turbo run build --filter=vs-code",
    "build:vs-code:ci": "turbo run build:ci --filter=vs-code",
    "build": "turbo run build && cd libs/tool-vault-packager && npm run build",
    "build:ci": "turbo run build:ci",
    "dev": "turbo run dev --filter=vs-code",
    "dev:web-components": "turbo run build --filter=@debrief/web-components",
    "dev:vs-code": "turbo run build --filter=vs-code^... && turbo run dev --filter=vs-code",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "turbo": "^2.5.6",
    "typescript": "^5.0.0"
  }
}
